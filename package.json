{
  "name": "ng-carousel-demo",
  "version": "1.4.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:lib": "ng test ng-carousel",
    "test:lib:coverage": "ng test ng-carousel --code-coverage --no-watch",
    "coverage-report": "cat ./coverage/ng-alert/lcov.info | codecov",
    "build:gh": "ng build --prod --base-href \"https://vagrantai-c.github.io/ng-carousel-cdk/\"",
    "publish-demo": "npx angular-cli-ghpages --dir dist/browser --repo https://GH_TOKEN@github.com/VagrantAI-c/ng-carousel-cdk.git",
    "copy:readme": "copyfiles ./README.md ./dist/ng-carousel",
    "build:lib": "ng build ng-carousel && npm run copy:readme",
    "publish:lib": "npx semantic-release",
    "semantic-release": "semantic-release",
    "adjust-version": "npm-run-all adjust-version:*",
    "adjust-version:lib": "rjp projects/ng-carousel/package.json version $VERSION",
    "adjust-version:main": "rjp package.json version $VERSION",
    "commit": "npx git-cz",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run ng-carousel-demo:server:production"
  },
  "private": true,
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "pkgRoot": "dist/ng-carousel",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "VERSION=${nextRelease.version} npm run adjust-version"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "projects/ng-carousel/package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "bezier-easing": "^2.1.0",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.24",
    "@angular-devkit/build-ng-packagr": "~0.803.24",
    "@angular/cli": "~8.3.24",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.2",
    "@semantic-release/npm": "^7.0.2",
    "@semantic-release/release-notes-generator": "^9.0.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.5.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.7.0",
    "angular-cli-ghpages": "^0.6.2",
    "codecov": "^3.6.4",
    "codelyzer": "^5.2.1",
    "copyfiles": "^2.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^5.7.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.3",
    "replace-json-property": "^1.4.3",
    "semantic-release": "^17.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "~8.6.2",
    "tsickle": "^0.38.0",
    "tslint": "~6.0.0",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VagrantAI-c/ng-carousel-cdk.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
